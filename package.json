{
    "name": "vscode-smart-column-indenter",
    "displayName": "Smart Column Indenter",
    "description": "(This extensions is not finished yet. Please, wait for the first release (version 1.0.0) to rate or write a review. I only published it because it's my first one and I had to learn how to publish it.) This extension adds support for indenting code into columns, to make your code more beautiful, readable, clear and with fewer lines of code.",
    "version": "0.0.3",
    "publisher": "lmcarreiro",
    "engines": {
        "vscode": "^1.17.0"
    },
    "bugs": {
        "url": "https://github.com/lmcarreiro/vscode-smart-column-indenter/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lmcarreiro/vscode-smart-column-indenter.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.smartColumnIndenter.indent2",
        "onCommand:extension.smartColumnIndenter.indentN",
        "onCommand:extension.smartColumnIndenter.removeLineBreaks"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            },
            {
                "command": "extension.smartColumnIndenter.indent2",
                "title": "Indent into 2 Columns"
            },
            {
                "command": "extension.smartColumnIndenter.indentN",
                "title": "Indent into N Columns"
            },
            {
                "command": "extension.smartColumnIndenter.removeLineBreaks",
                "title": "Remove Line Breaks"
            }
        ],
        "keybindings": [
            {
                "command": "extension.smartColumnIndenter.indent2",
                "key": "ctrl+i ctrl+2",
                "mac": "cmd+i cmd+2",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "command": "extension.smartColumnIndenter.indentN",
                "key": "ctrl+i ctrl+n",
                "mac": "cmd+i cmd+n",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "command": "extension.smartColumnIndenter.removeLineBreaks",
                "key": "ctrl+i ctrl+i",
                "mac": "cmd+i cmd+i",
                "when": "editorTextFocus && !editorReadonly"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.smartColumnIndenter.indent2",
                    "group": "2_indentation@1",
                    "when": "editorTextFocus && !editorReadonly"
                },
                {
                    "command": "extension.smartColumnIndenter.indentN",
                    "group": "2_indentation@2",
                    "when": "editorTextFocus && !editorReadonly"
                },
                {
                    "command": "extension.smartColumnIndenter.removeLineBreaks",
                    "group": "2_indentation@3",
                    "when": "editorTextFocus && !editorReadonly"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "smart-column-indenter": "0.0.1-dev.20171112.4"
    },
    "devDependencies": {
        "typescript": "latest",
        "vscode": "latest",
        "@types/node": "latest",
        "@types/mocha": "latest"
    }
}
